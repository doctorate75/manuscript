@article{reyesHelicobacterPyloriDeregulates2019,
  title = {{\emph{Helicobacter Pylori}} Deregulates t and b Cell Signaling to Trigger Immune Evasion},
  author = {Reyes, Victor E. and Peniche, Alex G.},
  year = {2019},
  journal = {Current Topics in Microbiology and Immunology},
  volume = {421},
  pages = {229--265},
  issn = {0070-217X},
  doi = {10.1007/978-3-030-15138-6_10},
  abstract = {Helicobacter pylori is a prevalent human pathogen that successfully establishes chronic infection, which leads to clinically significant gastric diseases including chronic gastritis, peptic ulcer disease (PUD), and gastric cancer (GC). H. pylori is able to produce a persistent infection due in large part to its ability to hijack the host immune response. The host adaptive immune response is activated to strategically and specifically attack pathogens and normally clears them from the infected host. Since B and T lymphocytes are central mediators of adaptive immunity, in this chapter we review their development and the fundamental mechanisms regulating their activation in order to understand how some of the normal processes are subverted by H. pylori. In this review, we place particular emphasis on the CD4+ T cell responses, their subtypes, and regulatory mechanisms because of the expanding literature in this area related to H. pylori. T lymphocyte differentiation and function are finely orchestrated through a series of cell-cell interactions, which include immune checkpoint receptors. Among the immune checkpoint receptor family, there are some with inhibitory properties that are exploited by tumor cells to facilitate their immune evasion. Gastric epithelial cells (GECs), which act as antigen-presenting cells (APCs) in the gastric mucosa, are induced by H. pylori to express immune checkpoint receptors known to sway T lymphocyte function and thus circumvent effective T effector lymphocyte responses. This chapter reviews these and other mechanisms used by H. pylori to interfere with host immunity in order to persist.},
  langid = {english},
  pmcid = {PMC7050993},
  pmid = {31123892},
  keywords = {B-Lymphocytes,Co-inhibitory receptors,Gastric Mucosa,H. pylori,Helicobacter Infections,Helicobacter pylori,Humans,Immune checkpoint regulators,Immune evasion,Immune Evasion,immune tolerance,Lymphocyte development,Reprogramming,review,T-Lymphocytes},
  file = {/home/ffs/Zotero/storage/53XUBKUY/Reyes and Peniche - 2019 - iHelicobacter pylorii deregulates t and b cel.pdf}
}

