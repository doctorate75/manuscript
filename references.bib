  keywords = {Bacterial Load,Biopsy,diagnosis,Email sent,Female,Gastric Mucosa,H. pylori,Helicobacter Infections,Helicobacter pylori,Human,Male,PCR,Predictive Value of Tests,primer,Prospective Studies,Real-Time Polymerase Chain Reaction,Sensitivity and Specificity},
