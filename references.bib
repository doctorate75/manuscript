@article{kwokHelicobacterExploitsIntegrin2007,
  title = {{\emph{Helicobacter}} Exploits Integrin for Type {{IV}} Secretion and Kinase Activation},
  author = {Kwok, Terry and Zabler, Dana and Urman, Sylwia and Rohde, Manfred and Hartig, Roland and Wessler, Silja and Misselwitz, Rolf and Berger, J{\"u}rgen and Sewald, Norbert and K{\"o}nig, Wolfgang and Backert, Steffen},
  year = {2007},
