@article{kandulskiHelicobacterPyloriInfection2008,
  title = {{\emph{Helicobacter Pylori}} Infection: A Clinical Overview},
  shorttitle = {Helicobacter Pylori Infection},
  author = {Kandulski, A and Selgrad, M and Malfertheiner, P},
  year = {2008},
