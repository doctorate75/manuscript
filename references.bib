  pmid = {22512976},
  keywords = {Adjuvants; Immunologic,AhpC,Alum Compounds,Animals,Antibodies; Bacterial,Bacterial Vaccines,Disease Models; Animal,Female,Gene Expression,Glycoproteins,H. pylori,Helicobacter Infections,Helicobacter pylori,Human,Immunoglobulin G,Mice,Mice; Inbred C57BL,mouse,Peroxiredoxins,Pichia,Recombinant Proteins,review,vac candidate,vaccine,Vaccines; Subunit,Vaccines; Synthetic},
