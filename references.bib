  pmid = {12823823},
  keywords = {Antigens; Bacterial,Bacterial Adhesion,Bacterial Proteins,Biological Transport,CagA,CagT,Cell Membrane Structures,Cells; Cultured,electron microscopy,EM,Epithelial Cells,FESEM,Genomic Islands,H. pylori,Helicobacter pylori,Immunohistochemistry,isogenic,M2,photo,Recombinant Fusion Proteins,T4SS,VirB7},
