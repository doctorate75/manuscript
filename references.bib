  title = {Identification of Glycan Structure Alterations on Cell Membrane Proteins in {{Desoxyepothilone B}} Resistant Leukemia Cells},
  author = {Nakano, Miyako and Saldanha, Rohit and G{\"o}bel, Anja and Kavallaris, Maria and Packer, Nicolle H.},
