@article{leeIsogenicVariationHelicobacter2005,
  title = {Isogenic Variation of {{{\emph{Helicobacter}}}}{\emph{ Pylori}} Strain Resulting in Heteroresistant Antibacterial Phenotypes in a Single Host {\emph{in Vivo}}},
  author = {Lee, Yong Chan and Lee, Sun-Young and Pyo, Jae Hee and Kwon, Dong H. and Rhee, Jong Chul and Kim, Jae J.},
  year = {2005},
