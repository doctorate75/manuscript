@article{tanHelicobacterPyloriGastritis2011,
  title = {{\emph{Helicobacter Pylori}} and Gastritis: Untangling a Complex Relationship 27 Years On},
  shorttitle = {Helicobacter Pylori and Gastritis},
  author = {Tan, Victoria P Y and Wong, Benjamin C Y},
  year = {2011},
  month = jan,
  journal = {Journal of gastroenterology and hepatology},
  volume = {26 Suppl 1},
  pages = {42--45},
  issn = {1440-1746},
  doi = {10.1111/j.1440-1746.2010.06593.x},
  abstract = {Since its' introduction by Warren and Marshall 27 years ago, Helicobacter pylori (HP) has become the linchpin in our understanding of important gastric conditions including gastritis, intestinal metaplasia (IM), gastric/duodenal ulcers (GU/DU), Mucosal Associated Lymphoid Tumour (MALToma) and gastric cancer. Initially named Campylobacter pyloridis, it was re-named HP when biochemical and genetic characterization of the organism showed that it was not a member of the Campylobacter genus. The finding in 1983 was seminal. It is now recognized that HP is the most common chronic human bacterial infection and it is the most common cause of gastritis. It is strongly implicated in the development of peptic ulcer disease and gastric neoplasms. In the years since its' discovery, much headway has been made in the understanding of this ubiquitous organism that had remained elusive, with much work focused on eradication, in part driven by pharmaceutical research and development. Standard triple therapy emerged to eradicate HP. However, with the emergence of HP resistance, newer regimes have been put forth that include quadruple therapy, sequential therapy and a dizzying array of other combinations bent on eradicating HP. Much less is known about the natural history of HP, the different faces of HP internationally, HP eradication and its effect on gastritis, IM, GU/DU and gastric cancer. This review will address the changing face of HP in 2011.},
  langid = {english},
  pmid = {21199513},
  keywords = {Anti-Bacterial Agents,Disease Progression,Drug Therapy; Combination,gastritis,H. pylori,Helicobacter Infections,Helicobacter pylori,History; 20th Century,History; 21st Century,Human,Peptic Ulcer,Proton Pump Inhibitors,review,Risk Assessment,Risk Factors,Stomach Neoplasms,Treatment Outcome},
  file = {/home/ffs/Zotero/storage/UTBJDSPR/Tan and Wong - 2011 - iHelicobacter pylorii and gastritis Untangli.pdf}
}

@article{tanHelicobacterPyloriPerturbs2011,
  title = {{\emph{Helicobacter Pylori}} Perturbs Iron Trafficking in the Epithelium to Grow on the Cell Surface},
  author = {Tan, Shumin and Noto, Jennifer M and {Romero-Gallo}, Judith and Peek, Jr, Richard M and Amieva, Manuel R},
  year = {2011},
  month = may,
  journal = {PLoS Pathogens},
  volume = {7},
  number = {5},
  pages = {e1002050},
  issn = {1553-7374},
  doi = {10.1371/journal.ppat.1002050},
  abstract = {Helicobacter pylori (Hp) injects the CagA effector protein into host epithelial cells and induces growth factor-like signaling, perturbs cell-cell junctions, and alters host cell polarity. This enables Hp to grow as microcolonies adhered to the host cell surface even in conditions that do not support growth of free-swimming bacteria. We hypothesized that CagA alters host cell physiology to allow Hp to obtain specific nutrients from or across the epithelial barrier. Using a polarized epithelium model system, we find that isogenic {$\Delta$}cagA mutants are defective in cell surface microcolony formation, but exogenous addition of iron to the apical medium partially rescues this defect, suggesting that one of CagA's effects on host cells is to facilitate iron acquisition from the host. Hp adhered to the apical epithelial surface increase basolateral uptake of transferrin and induce its transcytosis in a CagA-dependent manner. Both CagA and VacA contribute to the perturbation of transferrin recycling, since VacA is involved in apical mislocalization of the transferrin receptor to sites of bacterial attachment. To determine if the transferrin recycling pathway is involved in Hp colonization of the cell surface, we silenced transferrin receptor expression during infection. This resulted in a reduced ability of Hp to colonize the polarized epithelium. To test whether CagA is important in promoting iron acquisition in vivo, we compared colonization of Hp in iron-replete vs. iron-deficient Mongolian gerbils. While wild type Hp and {$\Delta$}cagA mutants colonized iron-replete gerbils at similar levels, {$\Delta$}cagA mutants are markedly impaired in colonizing iron-deficient gerbils. Our study indicates that CagA and VacA act in concert to usurp the polarized process of host cell iron uptake, allowing Hp to use the cell surface as a replicative niche.},
  langid = {english},
  pmcid = {PMC3093365},
  pmid = {21589900},
  keywords = {Adaptation; Physiological,allrefs,Animals,Antigens; Bacterial,Bacterial Adhesion,Bacterial Proteins,Caco-2 Cells,CagA,Cell Line,Cell Membrane,Cell Polarity,cited,Dogs,Down-Regulation,Epithelium,Gastric Mucosa,Gerbillinae,H. pylori,Helicobacter Infections,Helicobacter pylori,Human,Intercellular Junctions,Iron,key reference,mouse,Receptors; Transferrin,Sequence Deletion,Signal Transduction,Transcytosis,Transferrin,vaca},
  file = {/home/ffs/Zotero/storage/5GW3K2QH/Tan et al. - 2011 - iHelicobacter pylorii perturbs iron trafficki.pdf}
}

@article{tanigawaAntiinflammatoryEffectTwo2004,
  title = {Anti-Inflammatory Effect of Two Isoforms of {{COX}} in {{{\emph{H}}}}{\emph{. Pylori}}-Induced Gastritis in Mice: Possible Involvement of {{PGE2}}},
  shorttitle = {Anti-Inflammatory Effect of Two Isoforms of {{COX}} in {{H}}. Pylori-Induced Gastritis in Mice},
  author = {Tanigawa, Tetsuya and Watanabe, Toshio and Hamaguchi, Masaki and Sasaki, Eiji and Tominaga, Kazunari and Fujiwara, Yasuhiro and Oshitani, Nobuhide and Matsumoto, Takayuki and Higuchi, Kazuhide and Arakawa, Tetsuo},
  year = {2004},
  month = jan,
  journal = {American journal of physiology. Gastrointestinal and liver physiology},
  volume = {286},
  number = {1},
  pages = {G148-156},
  issn = {0193-1857},
  doi = {10.1152/ajpgi.00137.2003},
  abstract = {Neutrophil infiltration mediated by TNF-alpha is associated with various types of gastric injury, whereas PGs play a crucial role in gastric defense. We examined roles of two isoforms of cyclooxygenase (COX) and PGE2 in Helicobacter pylori-induced gastritis in mice. Mice infected with H. pylori were given selective COX-1 inhibitor SC-560 (10 mg/kg), selective COX-2 inhibitor NS-398 (10 mg/kg), or nonselective COX inhibitor indomethacin (2 mg/kg) with or without 16,16-dimethyl PGE2 for 1 wk. H. pylori infection increased levels of mRNA for COX-1 and -2 in gastric tissue by 1.2-fold and 3.3-fold, respectively, accompanied by a significant increase in PGE2 production by gastric tissue. H. pylori infection significantly elevated MPO activity, a marker of neutrophil infiltration, and epithelial cell apoptosis in the stomach. SC-560 augmented MPO activity and epithelial cell apoptosis with associated reduction in PGE2 production, whereas NS-398 had the same effects without affecting PGE2 production. Inhibition of both COX-1 and -2 by indomethacin or concurrent treatment with SC-560 and NS-398 resulted in a stronger increase in MPO activity and apoptosis than inhibition of either COX-1 or -2 alone. H. pylori infection elevated TNF-alpha mRNA expression in the stomach, which was further increased by indomethacin. Effects of COX inhibitors on neutrophil infiltration, apoptosis, and TNF-alpha expression in H. pylori-infected mice were abolished by exogenous 16,16-dimethyl PGE2. In conclusion, PGE2 derived from either COX-1 or -2 is involved in regulation of gastric mucosal inflammation and contributes to maintenance of mucosal integrity during H. pylori infection via inhibition of TNF-alpha expression.},
  langid = {english},
  pmid = {12958020},
  keywords = {allrefs,Animals,Anti-Inflammatory Agents,Apoptosis,Blotting; Western,cited2,COX,Cyclooxygenase 1,Cyclooxygenase 2,Cyclooxygenase 2 Inhibitors,Cyclooxygenase Inhibitors,Dinoprostone,Epithelial Cells,Female,Gastric Mucosa,gastritis,H. pylori,Helicobacter Infections,Helicobacter pylori,Indomethacin,Isoenzymes,Membrane Proteins,Mice,Mice; Inbred C57BL,NSAID,PGE2,Prostaglandin-Endoperoxide Synthases,Reverse Transcriptase Polymerase Chain Reaction,RNA; Messenger,Thromboxane B2,Tumor Necrosis Factor-alpha},
  file = {/home/ffs/Zotero/storage/SQD6QM33/Tanigawa et al. - 2004 - Anti-inflammatory effect of two isoforms of COX in.pdf}
}

@article{tatoReconcilingIdEgo2008,
