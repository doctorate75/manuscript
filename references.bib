@article{kinnebrewInterleukin23Production2012,
  title = {Interleukin 23 Production by Intestinal {{CD103}}(+){{CD11b}}(+) Dendritic Cells in Response to Bacterial Flagellin Enhances Mucosal Innate Immune Defense},
  author = {Kinnebrew, Melissa A and Buffie, Charlie G and Diehl, Gretchen E and Zenewicz, Lauren A and Leiner, Ingrid and Hohl, Tobias M and Flavell, Richard A and Littman, Dan R and Pamer, Eric G},
  year = {2012},
