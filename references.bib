@article{collisonIL35mediatedInductionPotent2010,
  title = {{{IL-35-mediated}} Induction of a Potent Regulatory {{T}} Cell Population},
  author = {Collison, Lauren W. and Chaturvedi, Vandana and Henderson, Abigail L. and Giacomin, Paul R. and Guy, Cliff and Bankoti, Jaishree and Finkelstein, David and Forbes, Karen and Workman, Creg J. and Brown, Scott A. and Rehg, Jerold E. and Jones, Michael L. and Ni, Hsiao-Tzu and Artis, David and Turk, Mary Jo and Vignali, Dario A. A.},
  year = {2010},
