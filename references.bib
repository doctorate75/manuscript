@article{harbourSystemicImmunizationUnadjuvanted2008,
  title = {Systemic Immunization with Unadjuvanted Whole {{{\emph{Helicobacter}}}}{\emph{ Pylori}} Protects Mice against Heterologous Challenge},
  author = {Harbour, Stacey N and Every, Alison L and Edwards, Stirling and Sutton, Philip},
  year = {2008},
