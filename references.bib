  title = {{{IL-27}} Increases the Proliferation and Effector Functions of Human Na\"ive {{CD8}}+ {{T}} Lymphocytes and Promotes Their Development into {{Tc1}} Cells},
  author = {Schneider, Raphael and Yaneva, Teodora and Beauseigle, Diane and {El-Khoury}, Lama and Arbour, Nathalie},
  year = {2011},
  month = jan,
  journal = {European journal of immunology},
  volume = {41},
  number = {1},
  pages = {47--59},
  issn = {1521-4141},
  doi = {10.1002/eji.201040804},
  abstract = {IL-27 has been shown to exhibit both pro- and anti-inflammatory properties; it favors mouse na\"ive CD4(+) T-cell differentiation into Th1 cells to the detriment of Th17 and Th2 skewing and regulates IL-10 and IL-17 production by human CD4(+) T cells. Moreover, IL-27 promotes proliferation and cytotoxic functions of mouse CD8(+) T lymphocytes, but no data are available on human CD8(+) T cells. We investigated the impact of IL-27 on human CD8(+) T cells. In contrast to mouse T cells, the IL-27 receptor (IL-27R), composed of T cell cytokine receptor (TCCR) and gp130, was detected on a greater percentage of human CD8(+) than CD4(+) T cells and these proportions increased upon polyclonal activation. IL-27 induced rapid STAT1 and STAT3 signaling, enhanced STAT1 protein levels, and induced SOCS1 and SOCS3 expression in a STAT1-dependent manner by human CD8(+) T cells. Addition of IL-27 to {$\alpha$}-CD3-activated na\"ive CD8(+) T cells significantly increased T-box transcription factor expression levels, cell proliferation, and IFN-{$\gamma$} and granzyme B production leading to increased CD8(+) T-cell-mediated cytotoxicity. These results demonstrate that IL-27, a rapidly produced cytokine by activated APC, has a profound impact on human na\"ive CD8(+) T cells, driving them to become highly efficient Tc1 cells.},
  langid = {english},
  pmid = {21182076},
  keywords = {27paradox,allrefs,Antigens; CD3,Antigens; Neoplasm,Carrier Proteins,CD8,CD8-Positive T-Lymphocytes,Cell Proliferation,Cells; Cultured,cited,Cytokine Receptor gp130,Granzymes,Human,IL-27,Interferon-gamma,Interleukin-17,key reference,Lymphocyte Activation,STAT1 Transcription Factor,STAT3 Transcription Factor,STAT5 Transcription Factor,T-Box Domain Proteins,T-Lymphocytes; Cytotoxic,Tc1,Th1 Cells},
  file = {/home/ffs/Zotero/storage/88T7XFV4/Schneider et al. - 2011 - IL-27 increases the proliferation and effector fun.pdf}
}

@article{schreiberSpatialOrientationHelicobacter2004,
  title = {The Spatial Orientation of {{{\emph{Helicobacter}}}}{\emph{ Pylori}} in the Gastric Mucus},
  author = {Schreiber, S{\"o}ren and Konradt, Manuela and Groll, Claudia and Scheid, Peter and Hanauer, Guido and Werling, Hans-Otto and Josenhans, Christine and Suerbaum, Sebastian},
