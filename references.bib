  file = {/home/ffs/Zotero/storage/EUD4CAW7/Stumhofer et al. - 2007 - Interleukins 27 and 6 induce STAT3-mediated T cell.pdf}
}

@article{stumhoferRoleIL27p28Antagonist2010,
  title = {A Role for {{IL-27p28}} as an Antagonist of Gp130-Mediated Signaling},
  author = {Stumhofer, Jason S. and Tait, Elia D. and Iii, William J. Quinn and Hosken, Nancy and Spudy, Bj{\"o}rn and Goenka, Radhika and Fielding, Ceri A. and O'Hara, Aisling C. and Chen, Yi and Jones, Michael L. and Saris, Christiaan J. M. and {Rose-John}, Stefan and Cua, Daniel J. and Jones, Simon A. and Elloso, M. Merle and Gr{\"o}tzinger, Joachim and Cancro, Michael P. and Levin, Steven D. and Hunter, Christopher A.},
  year = {2010},
