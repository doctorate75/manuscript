  author = {Banerjee, S and {Medina-Fatimi}, A and Nichols, R and Tendler, D and Michetti, M and Simon, J and Kelly, C P and Monath, T P and Michetti, P},
  year = {2002},
  month = nov,
  journal = {Gut},
  volume = {51},
  number = {5},
  pages = {634--640},
  issn = {0017-5749},
  abstract = {BACKGROUND AND AIMS: Escherichia coli heat labile enterotoxin (LT) at doses of 5 micro g or 10 micro g has adjuvant activity for oral immunisation in humans infected with Helicobacter pylori, but causes severe diarrhoea. This study was undertaken to establish a safe and effective dose of LT, to confirm the safety of recombinant urease, and to compare the immunogenicity of orally compared with enterically delivered urease. METHODS: 42 healthy adults without present or past H pylori infection were randomised to receive 60 mg recombinant H pylori urease in soluble or in encapsulated form, given with doses of LT ranging from 0 micro g to 2.5 micro g. Four oral doses were administered at day 1, 8, 29, and 57. Specific IgG, IgA, and antibody secreting cells were measured as well as total alpha4beta7 integrin positive lymphocyte responses. RESULTS: Enterically delivered urease was well tolerated and no serious adverse events occurred. Mild diarrhoea (one to four loose stools) occurred after the first immunisation in 50\% (6 of 12) of the volunteers exposed to 2.5 micro g LT (p=0.06; paired t test, compared with baseline) but not in volunteers exposed to lower LT doses. Immune responses occurred in five (p=0.048; Fisher's exact test), one, two, and one of six subjects exposed to 2.5 micro g, 0.5 micro g, 0.1 micro g, and no LT, respectively. Significant CD4(+), CD69(+), and CD45RO(hi) responses occurred over time among alpha4beta7(hi) lymphocytes in volunteers receiving 2.5 micro g LT. Enterically delivered urease induced higher lymphocyte responses than soluble urease. CONCLUSIONS: The safety of H pylori urease is confirmed. Oral LT may conserve its adjuvant activity at low doses with minimal side effects.},
  langid = {english},
  pmcid = {PMC1773429},
  pmid = {12377799},
  keywords = {Adjuvants; Immunologic,Administration; Oral,Adult,allrefs,Analysis of Variance,Bacterial Toxins,cited,clinical-trial-vaccine,Delayed-Action Preparations,Diarrhea,Double-Blind Method,Enterotoxins,Escherichia coli Proteins,Female,H. pylori,Helicobacter Infections,Helicobacter pylori,Human,Male,Middle Aged,Statistics; Nonparametric,T-Lymphocytes,urease,Urease,vac candidate,vaccine},
  file = {/home/ffs/Zotero/storage/65NXMQCX/Banerjee et al. - 2002 - Safety and efficacy of low dose Escherichia coli e.pdf}
}

@article{bansilInfluenceMucusMicrostructure2013,
  title = {The Influence of Mucus Microstructure and Rheology in {{{\emph{Helicobacter}}}}{\emph{ Pylori}} Infection},
  author = {Bansil, Rama and Celli, Jonathan P and Hardcastle, Joseph M and Turner, Bradley S},
  year = {2013},
  journal = {Frontiers in immunology},
  volume = {4},
  pages = {310},
  issn = {1664-3224},
  doi = {10.3389/fimmu.2013.00310},
  abstract = {The bacterium Helicobacter pylori (H. pylori), has evolved to survive in the highly acidic environment of the stomach and colonize on the epithelial surface of the gastric mucosa. Its pathogenic effects are well known to cause gastritis, peptic ulcers, and gastric cancer. In order to infect the stomach and establish colonies on the mucus epithelial surface, the bacterium has to move across the gel-like gastric mucus lining of the stomach under acidic conditions. In this review we address the question of how the bacterium gets past the protective mucus barrier from a biophysical perspective. We begin by reviewing the molecular structure of gastric mucin and discuss the current state of understanding concerning mucin polymerization and low pH induced gelation. We then focus on the viscoelasticity of mucin in view of its relevance to the transport of particles and bacteria across mucus, the key first step in H. pylori infection. The second part of the review focuses on the motility of H. pylori in mucin solutions and gels, and how infection with H. pylori in turn impacts the viscoelastic properties of mucin. We present recent microscopic results tracking the motion of H. pylori in mucin solutions and gels. We then discuss how the biochemical strategy of urea hydrolysis required for survival in the acid is also relevant to the mechanism that enables flagella-driven swimming across the mucus gel layer. Other aspects of the influence of H. pylori infection such as, altering gastric mucin expression, its rate of production and its composition, and the influence of mucin on factors controlling H. pylori virulence and proliferation are briefly discussed with references to relevant literature.},
  langid = {english},
  pmcid = {PMC3794295},
  pmid = {24133493},
  keywords = {allrefs,cited,H. pylori,mechanism,motility,mucin,mucin types,mucus,review,rheology,urease,virulence},
  file = {/home/ffs/Zotero/storage/36P2RZ5F/Bansil et al. - 2013 - The influence of mucus microstructure and rheology.pdf}
}

