  title = {Aberrant Expression of Gland-Type Gastric Mucin in the Surface Epithelium of {{{\emph{Helicobacter}}}}{\emph{ Pylori}}-Infected Patients},
  author = {Byrd, J C and Yan, P and Sternberg, L and Yunker, C K and Scheiman, J M and Bresalier, R S},
