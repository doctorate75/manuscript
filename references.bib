@article{michettiOralImmunizationUrease1999,
  title = {Oral Immunization with Urease and {{{\emph{Escherichia}}}}{\emph{ Coli}} Heat-Labile Enterotoxin Is Safe and Immunogenic in {{{\emph{Helicobacter}}}}{\emph{ Pylori}}-Infected Adults},
  author = {Michetti, P and Kreiss, C and Kotloff, K L and Porta, N and Blanco, J L and Bachmann, D and Herranz, M and Saldinger, P F and {Corth{\'e}sy-Theulaz}, I and Losonsky, G and Nichols, R and Simon, J and Stolte, M and Ackerman, S and Monath, T P and Blum, A L},
  year = {1999},
  month = apr,
  journal = {Gastroenterology},
  volume = {116},
  number = {4},
  pages = {804--812},
  issn = {0016-5085},
  abstract = {BACKGROUND \& AIMS: Oral immunization with Helicobacter pylori urease can cure Helicobacter infection in animals. As a step toward therapeutic immunization in humans, the safety and immunogenicity of oral immunization with recombinant H. pylori urease were tested in H. pylori-infected adults. METHODS: Twenty-six H. pylori-infected volunteers were randomized in a double-blind study to four weekly oral doses of 180, 60, or 20 mg of urease with 5 microg heat-labile enterotoxin of Escherichia coli (LT), LT alone, or placebo. Side effects and immune responses were evaluated weekly after immunization, and gastric biopsy specimens were obtained after 1 month and 6 months for histology and quantitative cultures. RESULTS: Diarrhea was noted in 16 of 24 (66\%) of the volunteers who completed the study. Antiurease serum immunoglobulin A titers increased 1. 58-fold +/- 0.37-fold and 3.66-fold +/- 1.5-fold (mean +/- SEM) after immunization with 60 and 180 mg urease, respectively, whereas no change occurred in the placebo +/- LT groups (P = 0.005). Circulating antiurease immunoglobulin A-producing cells increased in volunteers exposed to urease compared with placebo (38.9 +/- 13. 6/10(6) vs. 5.4 +/- 3.1; P = 0.018). Eradication of H. pylori infection was not observed, but urease immunization induced a significant decrease in gastric H. pylori density. CONCLUSIONS: H. pylori urease with LT is well tolerated and immunogenic in H. pylori-infected individuals. An improved vaccine formulation may induce curative immunity.},
  langid = {english},
  pmid = {10092302},
  keywords = {Administration; Oral,Adult,allrefs,askPDF,Bacterial Toxins,Bacterial Vaccines,cited,clinical-trial-vaccine,Double-Blind Method,Enterotoxins,Escherichia coli Proteins,Female,H. pylori,Helicobacter Infections,Helicobacter pylori,Human,Immunization,Immunoglobulin A,Male,Middle Aged,Stomach,urease,Urease,vac candidate,vaccine}
}

@article{migitaInductionInterleukin23P192010,
  title = {Induction of Interleukin-23 P19 by Serum Amyloid {{A}} ({{SAA}}) in Rheumatoid Synoviocytes},
  author = {Migita, K and Koga, T and Torigoshi, T and Motokawa, S and Maeda, Y and Jiuchi, Y and Izumi, Y and Miyashita, T and Nakamura, M and Komori, A and Ishibashi, H},
  year = {2010},
