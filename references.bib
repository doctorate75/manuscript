  keywords = {Alleles,Bacterial Proteins,Base Sequence,DNA Primers,Flagellin,H. pylori,Helicobacter pylori,Molecular Sequence Data,panmictic,Phylogeny,recombination,Recombination; Genetic,Species Specificity,transmission},
  file = {/home/ffs/Zotero/storage/2P58S626/Suerbaum et al. - 1998 - Free recombination within Helicobacter pylori.pdf}
}

