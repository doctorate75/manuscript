  pmid = {19696666},
  keywords = {Amoxicillin,Bismuth,clarithromycin,diagnosis,Drug Resistance; Bacterial,Drug Therapy; Combination,Fluoroquinolones,H. pylori,Helicobacter Infections,Helicobacter pylori,Human,Metronidazole,Ofloxacin,Practice Guidelines as Topic,print,Proton Pump Inhibitors,Rifabutin,treatment,Treatment Failure},
