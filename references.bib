  keywords = {allrefs,cited,cytokine,IL-17,IL-23,Th1,Th17},
  file = {/home/ffs/Zotero/storage/6UUUJ8XC/Taherian et al. - 2014 - The role of interleukin-23 in stability of in vitr.pdf}
}

