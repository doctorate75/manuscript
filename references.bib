@article{shirouzonoPreparationCrystallizationPreliminary2012,
  title = {Preparation, Crystallization and Preliminary {{X-ray}} Diffraction Studies of the Glycosylated Form of Human Interleukin-23},
  author = {Shirouzono, Takumi and Chirifu, Mami and Nakamura, Chiharu and Yamagata, Yuriko and Ikemizu, Shinji},
