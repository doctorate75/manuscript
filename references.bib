  author = {Chen, Qi and Ghilardi, Nico and Wang, Hua and Baker, Thad and Xie, Ming-Hong and Gurney, Austin and Grewal, Iqbal S. and de Sauvage, Frederic J.},
  year = {2000},
  month = oct,
  journal = {Nature},
  volume = {407},
  number = {6806},
  pages = {916--920},
  issn = {0028-0836},
  doi = {10.1038/35038103},
  abstract = {On antigen challenge, T-helper cells differentiate into two functionally distinct subsets, Th1 and Th2, characterized by the different effector cytokines that they secrete. Th1 cells produce interleukin (IL)-2, interferon- (IFN-) and lymphotoxin-, which mediate pro-inflammatory functions critical for the development of cell-mediated immune responses, whereas Th2 cells secrete cytokines such as IL-4, IL-5 and IL-10 that enhance humoral immunity. This process of T-helper cell differentiation is tightly regulated by cytokines. Here we report a new member of the type I cytokine receptor family, designated T-cell cytokine receptor (TCCR). When challenged in vivo with protein antigen, TCCR-deficient mice had impaired Th1 response as measured by IFN- production. TCCR-deficient mice also had increased susceptibility to infection with an intracellular pathogen, Listeria monocytogenes. In addition, levels of antigen-specific immunoglobulin-2a, which are dependent on Th1 cells, were markedly reduced in these mice. Our results demonstrate the existence of a new cytokine receptor involved in regulating the adaptive immune response and critical to the generation of a Th1 response.},
  copyright = {\textcopyright{} 2000 Nature Publishing Group},
  langid = {english},
  keywords = {27paradox,allrefs,astronomy,astrophysics,Biochemistry,bioinformatics,biology,Biotechnology,cancer,cell cycle,cell signalling,cited,climate change,Computational Biology,development,developmental biology,DNA,drug discovery,earth science,ecology,environmental science,evolution,evolutionary biology,expression source,functional genomics,genetics,Genomics,geophysics,IFNg,IL-27,Immunology,interdisciplinary science,key reference,life,marine biology,materials science,medical research,medicine,metabolomics,molecular biology,molecular interactions,Nanotechnology,nature,neurobiology,neuroscience,palaeobiology,pharmacology,physics,Proteomics,quantum physics,RNA,science,science news,science policy,Signal Transduction,structural biology,systems biology,Th1,transcriptomics,WSX-1},
  file = {/home/ffs/Zotero/storage/PF5VAZCX/Chen et al. - 2000 - Development of Th1-type immune responses requires .pdf}
