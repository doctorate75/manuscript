  author = {Jafarzadeh, Abdollah and Mirzaee, Vahid and {Ahmad-Beygi}, Hossain and Nemati, Maryam and Rezayati, Mohammad Taghi},
  year = {2009},
  month = may,
  journal = {Journal of digestive diseases},
