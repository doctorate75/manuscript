  pmid = {9247464},
  keywords = {Adult,Aged,Aged; 80 and over,Amino Acid Sequence,Antigens; CD15,Base Sequence,Biopsy,Concanavalin A,DNA,Enzyme-Linked Immunosorbent Assay,Epithelium,Female,Gastric Mucins,Gastric Mucosa,Gene Expression Regulation,H. pylori,Helicobacter Infections,Helicobacter pylori,Human,Immunohistochemistry,In Situ Hybridization,Male,Middle Aged,Molecular Sequence Data,MUC1,MUC5AC,MUC6},
