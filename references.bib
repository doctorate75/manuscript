@article{fischerSystematicMutagenesisHelicobacter2001,
  title = {Systematic Mutagenesis of the {{{\emph{Helicobacter}}}}{\emph{ Pylori Cag}} Pathogenicity Island: Essential Genes for {{CagA}} Translocation in Host Cells and Induction of Interleukin-8},
  shorttitle = {Systematic Mutagenesis of the {{Helicobacter}} Pylori Cag Pathogenicity Island},
