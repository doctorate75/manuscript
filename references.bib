  author = {S{\'a}nchez Zauco, Norma Ang{\'e}lica and Torres, Javier and P{\'e}rez Figueroa, Gloria Erandi and {\'A}lvarez Arellano, Lourdes and Camorlinga Ponce, Margarita and G{\'o}mez, Alejandro and Giono Cerezo, Silvia and Maldonado Bernal, Carmen},
  year = {2013},
