  pmcid = {PMC3880867},
  pmid = {24182612},
  keywords = {Anti-Bacterial Agents,clinical,Cost-Benefit Analysis,Costs and Cost Analysis,diagnosis,gastric cancer,H. pylori,Helicobacter Infections,Helicobacter pylori,Human,Mass Screening,prevention,review,screening,Stomach Neoplasms},
