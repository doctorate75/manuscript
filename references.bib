  file = {/home/ffs/Zotero/storage/BWSTECG2/Taga et al. - 1989 - Interleukin-6 triggers the association of its rece.pdf}
}

@article{taherianRoleInterleukin23Stability2014,
  title = {The Role of Interleukin-23 in Stability of in Vitro {{T}} Helper-17 Cells},
