  author = {McGuckin, Michael A and Every, Alison L and Skene, Caroline D and Linden, Sara K and Chionh, Yok Teng and Swierczak, Agnieszka and McAuley, Julie and Harbour, Stacey and Kaparakis, Maria and Ferrero, Richard and Sutton, Philip},
  year = {2007},
  month = oct,
  journal = {Gastroenterology},
