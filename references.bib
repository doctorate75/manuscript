  title = {Evaluation of Nested {{PCR}} in Detection of {{{\emph{Helicobacter}}}}{\emph{ Pylori}} Targeting a Highly Conserved Gene: {{HSP60}}},
  shorttitle = {Evaluation of Nested {{PCR}} in Detection of {{Helicobacter}} Pylori Targeting a Highly Conserved Gene},
  author = {Singh, Varsha and Mishra, Shrutkirti and Rao, G. R. K. and Jain, Ashok Kumar and Dixit, V. K. and Gulati, Anil Kumar and Mahajan, Divya and McClelland, Michael and Nath, Gopal},
