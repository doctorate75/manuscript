@article{deliosHelicobacterPyloriCells2005,
  title = {{\emph{Helicobacter Pylori}}, {{T}} Cells and Cytokines: The "Dangerous Liaisons"},
  shorttitle = {Helicobacter Pylori, {{T}} Cells and Cytokines},
  author = {D'Elios, Mario Milco and Amedei, Amedeo and Benagiano, Marisa and Azzurri, Annalisa and Del Prete, Gianfranco},
