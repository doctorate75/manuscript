@article{flachProinflammatoryCytokineGene2011,
  title = {Proinflammatory Cytokine Gene Expression in the Stomach Correlates with Vaccine-Induced Protection against {{{\emph{Helicobacter}}}}{\emph{ Pylori}} Infection in Mice: An Important Role for Interleukin-17 during the Effector Phase},
  shorttitle = {Proinflammatory Cytokine Gene Expression in the Stomach Correlates with Vaccine-Induced Protection against {{Helicobacter}} Pylori Infection in Mice},
  author = {Flach, Carl-Fredrik and {\"O}stberg, Anna Karin and Nilsson, Anne-Therese and Malefyt, Rene De Waal and Raghavan, Sukanya},
  year = {2011},
  month = feb,
  journal = {Infection and immunity},
  volume = {79},
  number = {2},
  pages = {879--886},
  issn = {1098-5522},
  doi = {10.1128/IAI.00756-10},
  abstract = {CD4(+) T cells have been shown to be essential for vaccine-induced protection against Helicobacter pylori infection in mice. Less is known about the relative contributions of individual cell subpopulations, such as T(h)1 and T(h)17 cells, and their associated cytokines. The aim of the present study was to find immune correlates to vaccine-induced protection and further study their role in protection against H. pylori infection. Immunized and unimmunized mice were challenged with H. pylori, and immune responses were compared. Vaccine-induced protection was assessed by measuring H. pylori colonization in the stomach. Gastric gene expression of T(h)1- and/or T(h)17-associated cytokines was analyzed by quantitative PCR, and contributions of individual cytokines to protection were evaluated by antibody-mediated in vivo neutralization. By analyzing immunized and unimmunized mice, a significant inverse correlation between the levels of interleukin-12p40 (IL-12p40), tumor necrosis factor alpha (TNF), gamma interferon (IFN-{$\gamma$}), and IL-17 gene expression and the number of H. pylori bacteria in the stomachs of individual animals after challenge could be demonstrated. In a kinetic study, upregulation of TNF, IFN-{$\gamma$}, and IL-17 coincided with vaccine-induced protection at 7 days after H. pylori challenge and was sustained for at least 21 days. In vivo neutralization of these cytokines during the effector phase of the immune response revealed a significant role for IL-17, but not for IFN-{$\gamma$} or TNF, in vaccine-induced protection. In conclusion, although both T(h)1- and T(h)17-associated gene expression in the stomach correlate with vaccine-induced protection against H. pylori infection, our study indicates that mainly T(h)17 effector mechanisms are of critical importance to protection.},
  langid = {english},
  pmcid = {PMC3028834},
  pmid = {21078851},
  keywords = {17clear,allrefs,Animals,Bacterial Vaccines,cited,Cytokines,Female,first reference,Gene Expression Regulation,H. pylori,Helicobacter Infections,Helicobacter pylori,IL-17,Interferon-gamma,Interleukin-17,Mice,Mice; Inbred C57BL,mouse,Stomach,Th1,Th17,Tumor Necrosis Factor-alpha,vac candidate,vaccine},
  file = {/home/ffs/Zotero/storage/E9TAZKRV/Flach et al. - 2011 - Proinflammatory Cytokine Gene Expression in the St.pdf}
}

@article{flachTruncatedFormHpaA2011,
  title = {A Truncated Form of {{HpaA}} Is a Promising Antigen for Use in a Vaccine against {{{\emph{Helicobacter}}}}{\emph{ Pylori}}},
  author = {Flach, Carl-Fredrik and Svensson, Natascha and Blomquist, Margareta and Ekman, Annelie and Raghavan, Sukanya and Holmgren, Jan},
  year = {2011},
  month = feb,
  journal = {Vaccine},
  volume = {29},
  number = {6},
  pages = {1235--1241},
  issn = {1873-2518},
  doi = {10.1016/j.vaccine.2010.11.088},
  abstract = {HpaA is a Helicobacter pylori-specific lipoprotein that has been shown to be an effective protective antigen for mucosal vaccination against H. pylori infection in mice. However, detergents are needed for the purification of full-length HpaA (HpaA(full)), which might confer toxicity, thus making HpaA(full) unsuitable for use in a human vaccine. We here describe a recombinantly produced truncated version of HpaA (HpaA(trunc)), which is easily purified without the use of detergents. Evaluation in the murine H. pylori infection model showed that sublingual immunization with HpaA(trunc) was equally immunogenic and protective as immunization with HpaA(full). Immunization with a combination of HpaA(trunc) and recombinant UreB protein induced strong immune responses to both antigens and importantly had a strong synergistic effect on protection, associated with synergistically increased expression of IL-17 in the stomach. Notably, sublingual immunization with HpaA(trunc) and UreB was superior to corresponding intragastric immunization with regard to the level of protection induced. In conclusion, HpaA(trunc) is a promising, readily produced, non-toxic recombinant antigen for inclusion in a mucosal vaccine against H. pylori infection, which may preferably be given sublingually together with UreB.},
  langid = {english},
  pmid = {21147129},
  keywords = {Adhesins; Bacterial,Animals,Antibodies; Bacterial,Antigens; Bacterial,Bacterial Vaccines,Cell Proliferation,Disease Models; Animal,Female,H. pylori,Helicobacter Infections,Helicobacter pylori,Histocytochemistry,HpaA,Lymph Nodes,Lymphocytes,Mice,Mice; Inbred C57BL,mouse,Recombinant Proteins,Stomach,vac candidate,vaccine,Vaccines; Synthetic},
