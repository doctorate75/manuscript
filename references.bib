}

@article{quiding-jarbrinkEnhancedM1Macrophage2010,
  title = {Enhanced {{M1}} Macrophage Polarization in Human {{{\emph{Helicobacter}}}}{\emph{ Pylori}}-Associated Atrophic Gastritis and in Vaccinated Mice},
