@article{sugaiRunx3RequiredFull2011,
  title = {Runx3 Is Required for Full Activation of Regulatory {{T}} Cells to Prevent Colitis-Associated Tumor Formation},
  author = {Sugai, Manabu and Aoki, Koji and Osato, Motomi and Nambu, Yukiko and Ito, Kosei and Taketo, Makoto M. and Shimizu, Akira},
