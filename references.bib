  author = {Bunai, Keigo and Nozaki, Manabu and Hamano, Miyuki and Ogane, Shinya and Inoue, Taro and Nemoto, Tadashi and Nakanishi, Hiroshi and Yamane, Kunio},
  year = {2003},
