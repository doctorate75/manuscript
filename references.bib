@article{kuangTumoractivatedMonocytesPromote2010,
  title = {Tumor-Activated Monocytes Promote Expansion of {{IL-17-producing CD8}}+ {{T}} Cells in Hepatocellular Carcinoma Patients},
  author = {Kuang, Dong-Ming and Peng, Chen and Zhao, Qiyi and Wu, Yan and Zhu, Ling-Yan and Wang, Jianen and Yin, Xiao-Yu and Li, Lian and Zheng, Limin},
  year = {2010},
