@article{yannamIL23InfectionsInflammation2012,
  title = {{{IL-23}} in Infections, Inflammation, Autoimmunity and Cancer: Possible Role in {{HIV-1}} and {{AIDS}}},
  shorttitle = {{{IL-23}} in Infections, Inflammation, Autoimmunity and Cancer},
  author = {Yannam, Govardhana Rao and Gutti, Tanuja and Poluektova, Larisa Y},
