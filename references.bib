@article{rashidApicalMUC1Expression2013,
  title = {Apical {{MUC1}} Expression Revealed on the Foveolar Epithelium in {{{\emph{H}}}}{\emph{. Pylori}} Gastritis},
  author = {Rashid, M and Teixeira, A S and Qureshi, U and Pereira, S P and Novelli, M R and Swallow, D M},
