  pmcid = {PMC3030729},
  pmid = {21233255},
  keywords = {Adjuvants; Immunologic,Animals,discuss,Flow Cytometry,Gene Expression Regulation,IL-10,IL-17,IL-27,Interleukin-12,Interleukin-23,Intestines,key reference,Lipopolysaccharides,Mice,Mice; Inbred C57BL,Mice; Knockout,mouse,p35,p40,Receptors; Cytokine,Signal Transduction,T-Lymphocytes,Th17,Treg,WSX-1},
