  pmid = {17068223},
  keywords = {Alleles,allrefs,Case-Control Studies,Chromosomes; Human; Pair 1,cited,Cohort Studies,Colitis; Ulcerative,Crohn Disease,first reference,Genetic Markers,Genetic Predisposition to Disease,Genetic Testing,Genome; Human,Haplotypes,Human,IBD,IL23R,Interleukin-23,Jews,Linkage Disequilibrium,polymorphism,Polymorphism; Single Nucleotide,Receptors; Interleukin,Signal Transduction},
