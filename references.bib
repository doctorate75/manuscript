  author = {{Hoc{\`e}s de la Guardia}, Ana{\"i}s and Staedel, Cathy and Kaafarany, Itidal and Cl{\'e}ment, Aur{\'e}lien and Roubaud Baudron, Claire and M{\'e}graud, Francis and Lehours, Philippe},
