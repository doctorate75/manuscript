  author = {Wang, Xiaoqian and Wei, Yinxiang and Xiao, He and Liu, Xiaoling and Zhang, Yu and Han, Gencheng and Chen, Guojiang and Hou, Chunmei and Ma, Ning and Shen, Beifen and Li, Yan and Egwuagu, Charles E. and Wang, Renxi},
