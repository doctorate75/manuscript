@article{ajeComparativeStudyStool2010,
  title = {Comparative Study of Stool Antigen Test and Serology for {{{\emph{Helicobacter}}}}{\emph{ Pylori}} among {{Nigerian}} Dyspeptic Patients -- a Pilot Study},
  author = {Aje, A O and Otegbayo, J A and Odaibo, G N and Bojuwoye, B J},
