@article{bodgerGastricMucosalSecretion1997,
  title = {Gastric Mucosal Secretion of Interleukin-10: Relations to Histopathology, {{{\emph{Helicobacter}}}}{\emph{ Pylori}} Status, and Tumour Necrosis Factor-Alpha Secretion},
  shorttitle = {Gastric Mucosal Secretion of Interleukin-10},
