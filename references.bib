@article{mullerFoldingSwitchRegulates2019,
  title = {A Folding Switch Regulates Interleukin 27 Biogenesis and Secretion of Its {$\alpha$}-Subunit as a Cytokine},
  author = {M{\"u}ller, Stephanie I. and Friedl, Antonie and Aschenbrenner, Isabel and {Esser-von Bieren}, Julia and Zacharias, Martin and Devergne, Odile and Feige, Matthias J.},
  year = {2019},
  month = jan,
  journal = {Proceedings of the National Academy of Sciences of the United States of America},
  volume = {116},
  number = {5},
  pages = {1585--1590},
  issn = {1091-6490},
  doi = {10.1073/pnas.1816698116},
  abstract = {A common design principle of heteromeric signaling proteins is the use of shared subunits. This allows encoding of complex messages while maintaining evolutionary flexibility. How cells regulate and control assembly of such composite signaling proteins remains an important open question. An example of particular complexity and biological relevance is the interleukin 12 (IL-12) family. Four functionally distinct {$\alpha\beta$} heterodimers are assembled from only five subunits to regulate immune cell function and development. In addition, some subunits act as independent signaling molecules. Here we unveil key molecular mechanisms governing IL-27 biogenesis, an IL-12 family member that limits infections and autoimmunity. In mice, the IL-27{$\alpha$} subunit is secreted as a cytokine, whereas in humans only heterodimeric IL-27 is present. Surprisingly, we find that differences in a single amino acid determine if IL-27{$\alpha$} can be secreted autonomously, acting as a signaling molecule, or if it depends on heterodimerization for secretion. By combining computer simulations with biochemical experiments, we dissect the underlying structural determinants: a protein folding switch coupled to disulfide bond formation regulates chaperone-mediated retention versus secretion. Using these insights, we rationally change folding and assembly control for this protein. This provides the basis for a more human-like IL-27 system in mice and establishes a secretion-competent human IL-27{$\alpha$} that signals on its own and can regulate immune cell function. Taken together, our data reveal a close link between protein folding and immunoregulation. Insights into the underlying mechanisms can be used to engineer immune modulators.},
  langid = {english},
  pmcid = {PMC6358668},
  pmid = {30651310},
  keywords = {Animals,Autoimmunity,Cell Line,Cytokines,first reference,HEK293 Cells,Human,IL-27,immune engineering,interleukins,Interleukins,mice,Mice,protein assembly,protein folding,Protein Folding,protein quality control,Protein Subunits,Signal Transduction,toread},
  file = {/home/ffs/Zotero/storage/WLR6EUMF/Müller et al. - 2019 - A folding switch regulates interleukin 27 biogenes.pdf}
}

