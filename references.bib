  keywords = {Animals,Genomic Library,H. pylori,Helicobacter Infections,Helicobacter pylori,Mice,Mice; Inbred C57BL,mouse,Plasmids,Stomach,vac candidate,Vaccination,vaccine,Vaccines; DNA},
