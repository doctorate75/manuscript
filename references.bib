  shorttitle = {The Gastric Mucus Layers},
  author = {Phillipson, Mia and Johansson, Malin E V and Henriksn{\"a}s, Johanna and Petersson, Joel and Gendler, Sandra J and Sandler, Stellan and Persson, A Erik G and Hansson, Gunnar C and Holm, Lena},
  year = {2008},
