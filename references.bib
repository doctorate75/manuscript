  pmid = {21666695},
  keywords = {allrefs,Animals,cited2,Citrobacter rodentium,Colitis,Enterobacteriaceae Infections,Female,groupNodwTh17,IL-6,Immunity; Innate,Interleukin-17,Interleukin-6,Intestinal Mucosa,Intestines,Male,Mice,NLR,Nod1,Nod1 Signaling Adaptor Protein,Nod2,Nod2 Signaling Adaptor Protein,Salmonella Infections; Animal,Salmonella typhimurium,Th17,Th17 Cells},
