  title = {Antibody Production and {{Th1-biased}} Response Induced by an Epitope Vaccine Composed of {{Cholera}} Toxin {{B}} Unit and {{{\emph{Helicobacter}}}}{\emph{ Pylori}} {{Lpp20}} Epitopes},
  author = {Li, Yan and Chen, Zhongbiao and Ye, Jianbin and Ning, Lijun and Luo, Jun and Zhang, Lili and Jiang, Yin and Xi, Yue and Ning, Yunshan},
  year = {2016},
  month = jun,
  journal = {Helicobacter},
  volume = {21},
