  keywords = {allrefs,CD8,Cell Differentiation,cited2,DC,dectin-1,Dendritic Cells,groupDectin1wTh17,Human,Immunoglobulin A,Immunoglobulin G,Lectins; C-Type,Lipopolysaccharides,Membrane Proteins,Nerve Tissue Proteins,Th17},
