  file = {/home/ffs/Zotero/storage/86AH47CS/Maul und Zeitz - 2012 - Ulcerative colitis immune function, tissue fibros.pdf}
}

@article{mayerCuttingEdgeTbet2008,
  title = {Cutting Edge: {{T-bet}} and {{IL-27R}} Are Critical for in Vivo {{IFN-gamma}} Production by {{CD8 T}} Cells during Infection},
  shorttitle = {Cutting Edge},
  author = {Mayer, Katrin D and Mohrs, Katja and Reiley, William and Wittmer, Susan and Kohlmeier, Jacob E and Pearl, John E and Cooper, Andrea M and Johnson, Lawrence L and Woodland, David L and Mohrs, Markus},
  year = {2008},
  month = jan,
  journal = {Journal of immunology (Baltimore, Md.: 1950)},
  volume = {180},
  number = {2},
  pages = {693--697},
  issn = {0022-1767},
  abstract = {CD8+ T cells are a major source of IFN-gamma, a key effector cytokine in immune responses against many viruses and protozoa. Although the transcription factor T-bet is required for IFN-gamma expression in CD4+ T cells, it is reportedly dispensable in CD8+ T cells, where the transcription factor Eomesodermin is thought to be sufficient. The diverse functions of IFN-gamma are mediated through the IFN-gammaR and STAT1. In CD4+ T cells, STAT1 appears to be critical for the activation of T-bet and IFN-gamma, suggesting an IFN-gamma-dependent positive feedback loop. However, STAT1 can also be activated by other cytokines, including IL-27. In the present study we show that, in contrast to in vitro conditions and the prevailing paradigm, T-bet is critical for the in vivo IFN-gamma production by CD8+ T cells upon infection of mice with diverse pathogens. Whereas IFN-gammaR signals are dispensable for the T-bet-dependent IFN-gamma production, direct IL-27Ralpha signals are critical.},
  langid = {english},
  pmid = {18178806},
  keywords = {27paradox,allrefs,Animals,CD8-Positive T-Lymphocytes,cited,first reference,Human,IFNg,IL-27,Infection,Influenza; Human,Interferon-gamma,Mice,Mice; Mutant Strains,Receptors; Cytokine,STAT1 Transcription Factor,T-Box Domain Proteins,Tbet,Toxoplasmosis}
}

@article{mcaleerMechanismsControllingTh172011,
