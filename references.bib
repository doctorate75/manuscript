  pmid = {24491543},
  keywords = {allrefs,Azacitidine,Binding Sites,Cell Line; Tumor,cited2,CpG Islands,DNA Methylation,Enzyme Inhibitors,Gene Expression Regulation,H. pylori,Helicobacter pylori,Host-Pathogen Interactions,Human,Immunoblotting,mechanism,MUC1,Mucin-1,pathogenesis,Promoter Regions; Genetic,Protein Binding,Reverse Transcriptase Polymerase Chain Reaction,Stat3,STAT3 Transcription Factor,Stomach Neoplasms,Up-Regulation},
