@article{wartonDistancebasedMultivariateAnalyses2012,
  title = {Distance-Based Multivariate Analyses Confound Location and Dispersion Effects},
  author = {Warton, David I. and Wright, Stephen T. and Wang, Yi},
  year = {2012},
  journal = {Methods in Ecology and Evolution},
  volume = {3},
  number = {1},
  pages = {89--101},
  issn = {2041-210X},
  doi = {10.1111/j.2041-210X.2011.00127.x},
  abstract = {1. A critical property of count data is its mean\textendash variance relationship, yet this is rarely considered in multivariate analysis in ecology.2. This study considers what is being implicitly assumed about the mean\textendash variance relationship in distance-based analyses \textendash{} multivariate analyses based on a matrix of pairwise distances \textendash{} and what the effect is of any misspecification of the mean\textendash variance relationship.3. It is shown that distance-based analyses make implicit assumptions that are typically out-of-step with what is observed in real data, which has major consequences.4. Potential consequences of this mean\textendash variance misspecification are: confounding location and dispersion effects in ordinations; misleading results when trying to identify taxa in which an effect is expressed; failure to detect a multivariate effect unless it is expressed in high-variance taxa.5. Data transformation does not solve the problem.6. A solution is to use generalised linear models and their recent multivariate generalisations, which is shown here to have desirable properties.},
  copyright = {\textcopyright{} 2011 The Authors. Methods in Ecology and Evolution \textcopyright{} 2011 British Ecological Society},
  langid = {english},
  keywords = {Bray–Curtis distance,community composition,generalised linear models,mean–variance relationship,multi-dimensional scaling,Multivariate Analysis,PERMANOVA,R package,SIMPER,species-by-site data,vegan},
  file = {/home/ffs/Zotero/storage/CPBAH8HQ/Warton et al. - 2012 - Distance-based multivariate analyses confound loca.pdf}
}

