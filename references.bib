@article{raghavanSublingualImmunizationProtects2010,
  title = {Sublingual Immunization Protects against {{{\emph{Helicobacter}}}}{\emph{ Pylori}} Infection and Induces {{T}} and {{B}} Cell Responses in the Stomach},
  author = {Raghavan, Sukanya and Ostberg, Anna Karin and Flach, Carl-Fredrik and Ekman, Annelie and Blomquist, Margareta and Czerkinsky, Cecil and Holmgren, Jan},
  year = {2010},
  month = oct,
  journal = {Infection and immunity},
  volume = {78},
  number = {10},
