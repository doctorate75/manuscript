@article{duhenProductionInterleukin222009,
  title = {Production of Interleukin 22 but Not Interleukin 17 by a Subset of Human Skin-Homing Memory {{T}} Cells},
  author = {Duhen, Thomas and Geiger, Rebekka and Jarrossay, David and Lanzavecchia, Antonio and Sallusto, Federica},
  year = {2009},
  month = aug,
  journal = {Nature immunology},
  volume = {10},
