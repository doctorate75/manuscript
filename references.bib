  pmid = {21859949},
  keywords = {Mass Spectrometry,O-glycosylation,PVDF},
  file = {/home/ffs/Zotero/storage/ZS54H4J4/Nakano et al. - 2011 - Identification of Glycan Structure Alterations on .pdf}
}

