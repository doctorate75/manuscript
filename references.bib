  issn = {0022-1007, 1540-9538},
  doi = {10.1084/jem.185.5.825},
  abstract = {Back to TopAbstract Interleukin-12 (IL-12), a heterodimeric cytokine produced by activated monocytes and dendritic cells, plays a crucial role in regulating interferon (IFN)-{$\gamma$} production and in the generation of IFN\textendash{$\gamma$}\textendash producing T helper 1 (Th1) cells. Here we show that the IL-12 receptor (IL12R) {$\beta$}2 subunit, a recently cloned binding and signal transducing component of the IL-12R, is expressed on human Th1 but not Th2 clones and is induced during differentiation of human naive cells along the Th1 but not the Th2 pathway. IL-12 and type I but not type II interferons induce expression of the IL-12R {$\beta$}2 chain during in vitro T cell differentiation after antigen receptor triggering. The selective expression and regulation of the IL-12R {$\beta$}2 subunit may help to understand the basis of Th1/Th2 differentiation and may provide therapeutic options for altering the Th1/Th2 balance in several immuno-pathological conditions such as autoimmune diseases and allergies.},
  langid = {english},
  keywords = {allrefs,cited,expression source,IL-12,IL12RB2,key reference,Th1},
  file = {/home/ffs/Zotero/storage/V2GNNT5F/Rogge et al. - 1997 - Selective Expression of an Interleukin-12 Receptor.pdf}
}

@article{rohdeNovelSheathedSurface2003,
  title = {A Novel Sheathed Surface Organelle of the {{{\emph{Helicobacter}}}}{\emph{ Pylori}} Cag Type {{IV}} Secretion System},
  author = {Rohde, Manfred and P{\"u}ls, J{\"u}rgen and Buhrdorf, Renate and Fischer, Wolfgang and Haas, Rainer},
