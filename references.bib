}

@article{leeRelationshipUrokinasetypePlasminogen2004,
  title = {Relationship between Urokinase-Type Plasminogen Receptor, Interleukin-8 Gene Expression and Clinicopathological Features in Gastric Cancer},
  author = {Lee, Kyung Hee and Bae, Sung Hwa and Lee, Jae Lyun and Hyun, Myung Soo and Kim, Sang Hoon and Song, Sun Kyo and Kim, Hee Sun},
  year = {2004},
  journal = {Oncology},
  volume = {66},
