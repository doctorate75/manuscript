@article{rudnickaGeneticPolymorphismsInflammatory2019,
  title = {Genetic Polymorphisms in Inflammatory and Other Regulators in Gastric Cancer: Risks and Clinical Consequences},
  shorttitle = {Genetic Polymorphisms in Inflammatory and Other Regulators in Gastric Cancer},
  author = {Rudnicka, Karolina and Backert, Steffen and Chmiela, Magdalena},
  year = {2019},
  journal = {Current Topics in Microbiology and Immunology},
  volume = {421},
  pages = {53--76},
  issn = {0070-217X},
  doi = {10.1007/978-3-030-15138-6_3},
  abstract = {Helicobacter pylori infection is associated with the development of a chronic inflammatory response, which may induce peptic ulcers, gastric cancer (GC), and mucosa-associated lymphoid tissue (MALT) lymphoma. Chronic H. pylori infection promotes the genetic instability of gastric epithelial cells and interferes with the DNA repair systems in host cells. Colonization of the stomach with H. pylori is an important cause of non-cardia GC and gastric MALT lymphoma. The reduction of GC development in patients who underwent anti-H. pylori eradication schemes has also been well described. Individual susceptibility to GC development depends on the host's genetic predisposition, H. pylori virulence factors, environmental conditions, and geographical determinants. Biological determinants are urgently sought to predict the clinical course of infection in individuals with confirmed H. pylori infection. Possible candidates for such biomarkers include genetic aberrations such as single-nucleotide polymorphisms (SNPs) found in various cytokines/growth factors (e.g., IL-1{$\beta$}, IL-2, IL-6, IL-8, IL-10, IL-13, IL-17A/B, IFN-{$\gamma$}, TNF, TGF-{$\beta$}) and their receptors (IL-RN, TGFR), innate immunity receptors (TLR2, TLR4, CD14, NOD1, NOD2), enzymes involved in signal transduction cascades (PLCE1, PKLR, PRKAA1) as well as glycoproteins (MUC1, PSCA), and DNA repair enzymes (ERCC2, XRCC1, XRCC3). Bacterial determinants related to GC development include infection with CagA-positive (particularly with a high number of EPIYA-C phosphorylation motifs) and VacA-positive isolates (in particular s1/m1 allele strains). The combined genotyping of bacterial and host determinants suggests that the accumulation of polymorphisms favoring host and bacterial features increases the risk for precancerous and cancerous lesions in patients.},
  langid = {english},
  pmid = {31123885},
  keywords = {Gastric cancer,Gene polymorphism,Genetic Predisposition to Disease,Helicobacter Infections,Helicobacter pylori,Humans,Inflammation,Inflammation Mediators,Polymorphism; Single Nucleotide,SNP,Stomach Neoplasms,Virulence Factors}
}

@article{ruggeGastritisHistologyReport2011,
  title = {Gastritis: The Histology Report},
  shorttitle = {Gastritis},
  author = {Rugge, Massimo and Pennelli, Gianmaria and Pilozzi, Emanuela and Fassan, Matteo and Ingravallo, Giuseppe and Russo, Valentina M and Di Mario, Francesco and {Gruppo Italiano Patologi Apparato Digerente (GIPAD)} and {Societ\`a Italiana di Anatomia Patologica e Citopatologia Diagnostica/International Academy of Pathology, Italian division (SIAPEC/IAP)}},
  year = {2011},
  month = mar,
  journal = {Digestive and liver disease: official journal of the Italian Society of Gastroenterology and the Italian Association for the Study of the Liver},
  volume = {43 Suppl 4},
  pages = {S373-384},
  issn = {1878-3562},
  doi = {10.1016/S1590-8658(11)60593-8},
  abstract = {Gastritis is defined as inflammation of the gastric mucosa. In histological terms, it is distinguishable into two main categories, i.e. non-atrophic and atrophic. In the gastric mucosa, atrophy is defined as the loss of appropriate glands. There are several etiological types of gastritis, their different etiology being related to different clinical manifestations and pathological features. Atrophic gastritis (resulting mainly from long-standing Helicobacter pylori infection) is a major risk factor for the onset of (intestinal type) gastric cancer. The extent and site of the atrophic changes correlate significantly with the cancer risk. The current format for histology reporting in cases of gastritis fails to establish an immediate link between gastritis phenotype and risk of malignancy. Building on current knowledge of the biology of gastritis, an international group of pathologists [Operative Link for Gastritis Assessment (OLGA)] has proposed a system for reporting gastritis in terms of its stage (the OLGA Staging System): this system places the histological phenotypes of gastritis on a scale of progressively increasing gastric cancer risk, from the lowest (Stage 0) to the highest (Stage IV). The aim of this tutorial is to provide unequivocal information on how to standardize histology reports on gastritis in diagnostic practice.},
  langid = {english},
  pmid = {21459343},
  keywords = {allrefs,Biopsy,cited,Gastric Mucosa,gastritis,Gastritis; Atrophic,H. pylori,Helicobacter pylori,histopathology,Human,now,OLGA,Pathology,regeneration,review,Risk,Severity of Illness Index,SPEM,Stomach Neoplasms,Sydney},
  file = {/home/ffs/Zotero/storage/RBKSSRWZ/Rugge et al. - 2011 - Gastritis the histology report.pdf}
}

@article{ruggeOLGAStagingGastritis2008,
  title = {{{OLGA}} Staging for Gastritis: A Tutorial},
  shorttitle = {{{OLGA}} Staging for Gastritis},
  author = {Rugge, M and Correa, P and Di Mario, F and {El-Omar}, E and Fiocca, R and Geboes, K and Genta, R M and Graham, D Y and Hattori, T and Malfertheiner, P and Nakajima, S and Sipponen, P and Sung, J and Weinstein, W and Vieth, M},
  year = {2008},
