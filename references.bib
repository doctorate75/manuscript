  author = {Belda, Sof{\'i}a and Saez, Jes{\'u}s and Santib{\'a}{\~n}ez, Miguel and Rodr{\'i}guez, Juan Carlos and Galiana, Antonio and {Sola-Vera}, Javier and {Ruiz-Garc{\'i}a}, Montserrat and Brotons, Alicia and {L{\'o}pez-Girona}, Elena and Girona, Eva and Sillero, Carlos and Royo, Gloria},
  year = {2012},
