@article{liSystemicImmunizationEpitopebased2012,
  title = {Systemic Immunization with an Epitope-Based Vaccine Elicits a {{Th1-biased}} Response and Provides Protection against {{{\emph{Helicobacter}}}}{\emph{ Pylori}} in Mice},
  author = {Li, Hai-Bo and Zhang, Jin-Yong and He, Ya-Fei and Chen, Li and Li, Bin and Liu, Kai-Yun and Yang, Wu-Chen and Zhao, Zhuo and Zou, Quan-Ming and Wu, Chao},
  year = {2012},
