  pmid = {15238076},
  keywords = {Animals,Antibodies; Bacterial,Antigens; Bacterial,CD4-Positive T-Lymphocytes,Cell Count,Cell Division,Coculture Techniques,Cytokines,Epitopes,Flow Cytometry,gastritis,groupTregwHP,H. pylori,Helicobacter Infections,Helicobacter pylori,Immunization,Mice,Mice; Inbred C57BL,mouse,Receptors; Interleukin-2,Treg},
