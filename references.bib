@article{raghavanAntigenspecificVitroSuppression2004,
  title = {Antigen-Specific in Vitro Suppression of Murine {{{\emph{Helicobacter}}}}{\emph{ Pylori}}-Reactive Immunopathological {{T}} Cells by {{CD4CD25}} Regulatory {{T}} Cells},
