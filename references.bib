@article{uematsuRegulationHumoralCellular2008,
  title = {Regulation of Humoral and Cellular Gut Immunity by Lamina Propria Dendritic Cells Expressing {{Toll-like}} Receptor 5},
  author = {Uematsu, Satoshi and Fujimoto, Kosuke and Jang, Myoung Ho and Yang, Bo-Gie and Jung, Yun-Jae and Nishiyama, Mika and Sato, Shintaro and Tsujimura, Tohru and Yamamoto, Masafumi and Yokota, Yoshifumi and Kiyono, Hiroshi and Miyasaka, Masayuki and Ishii, Ken J and Akira, Shizuo},
