@article{qinghaiEffectInterleukin17AInterleukin17F2014,
  title = {Effect of Interleukin-{{17A}} and Interleukin-{{17F}} Gene Polymorphisms on the Risk of Gastric Cancer in a {{Chinese}} Population},
