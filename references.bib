  pmid = {31511508},
  keywords = {Animals,chaperone,Chlorocebus aethiops,COS Cells,Cysteine,Endoplasmic Reticulum,first reference,folding,Half-Life,Human,IL-12,Interleukin-23,Models; Biological,Molecular Chaperones,Protein Folding,Protein Stability,Protein Structure; Secondary,toread},
