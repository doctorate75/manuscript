@article{bustinVariabilityReverseTranscription2015,
  title = {Variability of the Reverse Transcription Step: Practical Implications},
  shorttitle = {Variability of the Reverse Transcription Step},
  author = {Bustin, Stephen and Dhillon, Harvinder S. and Kirvell, Sara and Greenwood, Christina and Parker, Michael and Shipley, Gregory L. and Nolan, Tania},
