@article{fukaseEffectEradicationHelicobacter2008,
  title = {Effect of Eradication of {{{\emph{Helicobacter}}}}{\emph{ Pylori}} on Incidence of Metachronous Gastric Carcinoma after Endoscopic Resection of Early Gastric Cancer: An Open-Label, Randomised Controlled Trial},
