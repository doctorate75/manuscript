  author = {Nuding, Sabine and Gersemann, Michael and Hosaka, Yoshio and Konietzny, Sabrina and Schaefer, Christian and Beisner, Julia and Schroeder, Bjoern O and Ostaff, Maureen J and Saigenji, Katunori and Ott, German and Schaller, Martin and Stange, Eduard F and Wehkamp, Jan},
  year = {2013},
