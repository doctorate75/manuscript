}

@article{jensenStructuralAnalysisOglycans2012,
  title = {Structural Analysis of {{N-}} and {{O-glycans}} Released from Glycoproteins},
  author = {Jensen, Pia H and Karlsson, Niclas G and Kolarich, Daniel and Packer, Nicolle H},
  year = {2012},
  month = jul,
  journal = {Nature protocols},
  volume = {7},
