  author = {Heidel, Florian and Lipka, Daniel B and Mirea, Fian K and Mahboobi, Siavosh and Grundler, Rebekka and Kancha, Rama K and Duyster, Justus and Naumann, Michael and Huber, Christoph and B{\"o}hmer, Frank D and Fischer, Thomas},
