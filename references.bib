  author = {B{\"a}ckhed, Fredrik and Rokbi, Bachra and Torstensson, Elisabeth and Zhao, Ying and Nilsson, Christina and Seguin, Delphine and Normark, Staffan and Buchan, Alison M J and {Richter-Dahlfors}, Agneta},
