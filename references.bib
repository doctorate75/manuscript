  file = {/home/ffs/Zotero/storage/ITJV7TPH/Romero-Adrián et al. - 2010 - Helicobacter pylori Bacterial Factors and the Rol.pdf;/home/ffs/Zotero/storage/BVAVP2BX/10.html}
