  title = {Interleukin-1 and {{IL-23}} Induce Innate {{IL-17}} Production from Gammadelta {{T}} Cells, Amplifying {{Th17}} Responses and Autoimmunity},
  author = {Sutton, Caroline E and Lalor, Stephen J and Sweeney, Cheryl M and Brereton, Corinna F and Lavelle, Ed C and Mills, Kingston H G},
