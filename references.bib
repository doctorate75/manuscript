  author = {Witowski, J and Pawlaczyk, K and Breborowicz, A and Scheuren, A and {Kuzlan-Pawlaczyk}, M and Wisniewska, J and Polubinska, A and Friess, H and Gahl, G M and Frei, U and J{\"o}rres, A},
