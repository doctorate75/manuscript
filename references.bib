@article{bensonDiscoveryMechanismUstekinumab2011,
  title = {Discovery and Mechanism of Ustekinumab: A Human Monoclonal Antibody Targeting {{Interleukin-12}} and {{Interleukin-23}} for Treatment of Immune-Mediated Disorders},
