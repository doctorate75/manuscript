  pmid = {25705648},
  keywords = {DNA; Bacterial,Genetic Variation,genome sequence,Genome; Bacterial,Genomic Islands,Genomics,H. pylori,Helicobacter Infections,Helicobacter pylori,Humans,Phylogeny,primer,Stomach,Stomach Neoplasms,toread,Virulence},
