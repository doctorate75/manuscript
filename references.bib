@article{liRUNX3ProtectsAcute2019,
  title = {{{RUNX3}} Protects against Acute Lung Injury by Inhibiting the {{JAK2}}/{{STAT3}} Pathway in Rats with Severe Acute Pancreatitis},
  author = {Li, S. and Cui, H.-Z. and Xu, C.-M. and Sun, Z.-W. and Tang, Z.-K. and Chen, H.-L.},
