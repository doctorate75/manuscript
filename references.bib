@article{traversoMorphologicGeneticBiochemical2010,
  title = {Morphologic, Genetic, and Biochemical Characterization of {{{\emph{Helicobacter}}}}{\emph{ Magdeburgensis}}, a Novel Species Isolated from the Intestine of Laboratory Mice},
  author = {Traverso, Francisco Rivas and Bohr, Ulrich R. M. and Oyarzabal, Omar A. and Rohde, Manfred and Clarici, Alexandra and Wex, Thomas and Kuester, Doerthe and Malfertheiner, Peter and Fox, James G. and Backert, Steffen},
  year = {2010},
  journal = {Helicobacter},
  volume = {15},
  number = {5},
