@article{muranskiTh17CellsAre2011,
  title = {Th17 Cells Are Long Lived and Retain a Stem Cell-like Molecular Signature},
  author = {Muranski, Pawel and Borman, Zachary A and Kerkar, Sid P and Klebanoff, Christopher A and Ji, Yun and {Sanchez-Perez}, Luis and Sukumar, Madhusudhanan and Reger, Robert N and Yu, Zhiya and Kern, Steven J and Roychoudhuri, Rahul and Ferreyra, Gabriela A and Shen, Wei and Durum, Scott K and Feigenbaum, Lionel and Palmer, Douglas C and Antony, Paul A and Chan, Chi-Chao and Laurence, Arian and Danner, Robert L and Gattinoni, Luca and Restifo, Nicholas P},
