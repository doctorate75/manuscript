@article{stumhoferInterleukins27Induce2007,
  title = {Interleukins 27 and 6 Induce {{STAT3-mediated T}} Cell Production of Interleukin 10},
  author = {Stumhofer, Jason S. and Silver, Jonathan S. and Laurence, Arian and Porrett, Paige M. and Harris, Tajie H. and Turka, Laurence A. and Ernst, Matthias and Saris, Christiaan J. M. and O'Shea, John J. and Hunter, Christopher A.},
  year = {2007},
